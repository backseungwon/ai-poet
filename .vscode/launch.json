      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "PATH": "C:\\Users\\baegs\\miniconda3\\envs\\langchain;${env:PATH}",
        "CONDA_DEFAULT_ENV": "langchain"
      }
    }
  ]
}