{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Streamlit with Conda",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/main.py"
